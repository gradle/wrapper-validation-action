{
  "name": "wrapper-validation-action",
  "version": "0.0.0",
  "private": true,
  "description": "Gradle Wrapper Validation Action",
  "main": "lib/main.js",
  "bin": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gradle/wrapper-validation-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Gradle Inc.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.4.0",
    "commander": "^8.2.0",
    "typed-rest-client": "1.8.4",
    "unhomoglyph": "1.0.6"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^12.20.33",
    "@typescript-eslint/parser": "4.28.4",
    "@vercel/ncc": "0.29.0",
    "eslint": "7.31.0",
    "eslint-plugin-github": "4.1.5",
    "eslint-plugin-jest": "24.4.0",
    "glob-parent": ">=5.1.2",
    "jest": "27.0.6",
    "js-yaml": "^4.1.0",
    "nock": "13.1.1",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "typescript": "4.0.8"
  }
}
